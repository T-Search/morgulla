{
	"compilerOptions": {
		"paths": {
			"$lib": [
				"src/lib"
			],
			"$lib/*": [
				"src/lib/*"
			],
			"$stores": [
				"src/stores"
			],
			"$stores/*": [
				"src/stores/*"
			],
			"$entities": [
				"src/entities"
			],
			"$entities/*": [
				"src/entities/*"
			],
		}
	},
	"extends": "./.svelte-kit/tsconfig.json",
}